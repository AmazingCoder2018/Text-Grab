<Application
    x:Class="Text_Grab.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Text_Grab"
    Startup="appStartup">
    <Application.Resources />
    <JumpList.JumpList>
        <JumpList>
            <JumpTask
                Title="Settings"
                Arguments="Settings"
                Description="Open Text Grab Settings" />
            <JumpTask
                Title="Grab Frame"
                Arguments="GrabFrame"
                Description="Open a Grab Frame" />
        </JumpList>
    </JumpList.JumpList>
</Application>
